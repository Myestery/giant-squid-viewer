{
  "name": "@myestery/giant-squid-viewer",
  "version": "1.0.6",
  "description": "Giant Squid Viewer for polkadot using the giant squid graphql endpoints and rpc of the chains",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Myestery/giant-squid-viewer.git"
  },
  "scripts": {
    "test": "jest --config jestconfig.json --detectOpenHandles --forceExit",
    "rollup": "rollup -c",
    "build": "npm run build:tailwind && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:tailwind": "tailwindcss -i ./tailwind.css -o ./styles/index.module.css",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\""
  },
  "prepare": "npm run build",
  "prepublishOnly": "npm test && npm run lint",
  "keywords": [
    "polkadot",
    "giant-squid"
  ],
  "author": "Chiwetelu Johnpaul (myestery)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "styles"
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": ">16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": ">16.0.0",
    "react-dom": ">16.0.0",
    "tailwindcss": "^3.3.1",
    "rollup": "^2.79.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@polkadot/api": "^10.3.4",
    "ethers": "^6.3.0",
    "graphql": "^16.6.0"
  }
}
